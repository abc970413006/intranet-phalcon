{
    "name": "denners777/api-phalcon",
    "description": "APP for application management in Phalcon PHP. Database: Oracle. Integrated with Whoops.",
    "version": "2.0.1",
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        }
    },
    "require": {
        "php": ">=5.6",
        "ext-phalcon": "2.*",
        "phalcon/incubator": "2.*",
        "aymanrb/php-unstructured-text-parser": "dev-master",
        "ilya/belt": "~2",
        "filp/whoops": "*",
        "phpoffice/phpexcel": "^1.1",
        "swiftmailer/swiftmailer": "^5.4",
        "respect/validation": "^1.1",
        "phalcongelist/breadcrumbs": "^1.2",
        "jbroadway/urlify": "*",
        "mpdf/mpdf": "dev-master",
        "cangelis/pdf": "2.*",
        "iio/libmergepdf": "~3.0",
        "kriswallsmith/assetic": "*",
        "vlucas/phpdotenv": "*",
        "rollbar/rollbar": "~0.18.2"
    },
    "require-dev": {
        "symfony/debug": "*",
        "symfony/var-dumper": "*",
        "devster/ubench": "1.1.*",
        "tracy/tracy": "*",
        "roave/security-advisories": "dev-master"
    },
    "authors": [{
            "name": "Denner Fernandes",
            "email": "denners777@hotmail.com",
            "homepage": "https://github.com/denners777/",
            "role": "Developer"
        }],
    "keywords": ["phalcon", "framework", "api", "app"],
    "type": "app",
    "license": "MIT",
    "homepage": "https://github.com/denners777/API-Phalcon"
